{
  "name": "tweetyoink",
  "version": "0.1.0",
  "description": "Capture tweets from Twitter/X for LLM analysis",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "watch": "vite build --watch",
    "watch:dev": "vite build --watch --mode development",
    "type-check": "tsc --noEmit",
    "generate-icons": "tsx scripts/generate-icons.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "server": "tsx test-server/server.ts",
    "server:async": "tsx test-server/async-server.ts"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.21",
    "@playwright/test": "^1.56.1",
    "@types/chrome": "^0.0.254",
    "@types/node": "^24.9.1",
    "playwright": "^1.56.1",
    "sharp": "^0.34.4",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.2"
  },
  "keywords": [
    "chrome-extension",
    "twitter",
    "x",
    "llm",
    "tweet-capture"
  ],
  "author": "",
  "license": "MIT"
}
